<%- include('./partials/headershow.ejs') %>
    <h1><%= seaFood.name %></h1>
    <div class="fishContainer">
        <img src="<%= seaFood.img %>" alt="">
        <p class="description"><%= seaFood.description %></p>
        <% if (seaFood.qty > 0) { %>
            <h4 class = qty><%= seaFood.qty %> lb remaining</h4>
            <h5 class="price">Price: $<%= seaFood.price %></h5>
        <% } else { %>
            <p class="out">OUT OF STOCK</p>
        <% } %>
    </div>
    <form action="<% seaFood %>?_method=DELETE" method="post">
        <button type="submit">DELETE</button>
    </form>
    <%- include('./partials/footer.ejs') %>
</body>
</html>